---
const { title } = Astro.props;
import './src/styles/global.css';
---
<!DOCTYPE html>
<html lang="id">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Hallo, ini adalah hasil saya belajar membuat website selama satu minggu">
    <link rel="shortcut icon" href="icon.png" type="image/x-icon">
    <title>{title}</title>

    <meta name="og:title" content="Hello World">
    <meta name="og:description" content="Hallo, ini adalah hasil saya belajar membuat website">
    <meta name="og:image" content="/favicon.svg">
    <meta name="og:url" content="https://rpnza.my.id/helloworld">
</head>
<body>
    <slot />
</body>
</html>